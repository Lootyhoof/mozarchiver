<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>mozarchiver@lootyhoof-pm</em:id>
    <em:version>2.1.0</em:version>
    <em:type>2</em:type>
    <em:multiprocessCompatible>false</em:multiprocessCompatible>
    <em:updateURL>https://raw.githubusercontent.com/Lootyhoof/mozarchiver/master/update.xml</em:updateURL>

    <!-- Target Application this extension can install into,
        with minimum and maximum supported versions. -->
    <!-- Applications using Firefox GUID -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <!--
            Pale Moon 30 and above will stop using its own GUID to
            "further improve extension compatibility"
          -->
        <em:minVersion>30.0.0a1</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <!-- Pale Moon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>27.0</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
     <!-- SeaMonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.46</em:minVersion>
        <em:maxVersion>2.49.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>MozArchiver</em:name>
    <em:description>Save exactly what you see, in a single file. MAFF (based on ZIP) is compact and exceptional for video and audio. MHT (Internet Explorer's format) allows easy exchange with IE users. Store many tabs, find original, and more!</em:description>
    <em:creator>Lootyhoof</em:creator>
    <em:developer>Paolo Amadini</em:developer>
    <em:developer>Christopher Ottley</em:developer>
    <em:contributor>Ueli Lezzi</em:contributor>
